{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/ping",
        "dest": "/app.js"
      },
      {
        "src": "/api/v1/user/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/api/v1/course/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/api/v1/payment/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/api/v1/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  